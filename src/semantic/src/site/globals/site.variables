/*******************************
     User Global Variables
*******************************/

@headerFont        : 'Montserrat', 'Helvetica Neue', Arial, Helvetica, sans-serif;
@pageFont          : 'Montserrat', 'Helvetica Neue', Arial, Helvetica, sans-serif;
@emSize   : 16px;
@importGoogleFonts : true;
@googleFontName    : 'Montserrat';
@googleFontSizes   : '200,200italic';
@googleFontFamily  : '@{googleFontName}:@{googleFontSizes}';

@green: #24B99B;

@primaryColor        : @green;
@linkColor           : @green;
@pageBackground      : @black;
